dropdown1.new("Epic Chest")
dropdown1.new("Rare Chest")
dropdown1.new("Uncommon Chest")
dropdown1.new("Common Chest")
dropdown1.event:Connect(function(item)
    chest_selected = item
    if item == "legendary" and settings.autochest == true then 
        autochestd()
    end
    if item == "epic" and settings.autochest == true then
        autochestd()
    end
    if item == "rare" and settings.autochest == true then 
        autochestd()
    end
    if item == "uncommon" and settings.autochest == true then 
        autochestd()
    end
    if item == "common" and settings.autochest == true then 
        autochestd()
    end
if item == "Legendary Chest" or "Epic Chest" or "Rare Chest" or "Uncommon Chest" or "Common chest" and settings.autochest == false then 
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "wayne.lua", -- Required
        Text = "please turn on auto open chest", -- Required
        Duration = 6
    })
end
end
end)

