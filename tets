    text = "autofarm",
})

local allstage = farming.new("switch", {
    text = "swithc!";
})
allstage.event:Connect(function(bool)
    settings.enabled = Bool 
    if settings.enabled then 
        autofarm()
    end
    if settings.enabled == false then 
        settings.enabled = false 
    end
    end
end)

